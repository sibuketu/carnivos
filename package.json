{
  "name": "primal-logic-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "build:skip-check": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "deploy": "cmd /c \"deploy.bat\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "preview": "vite preview",
    "save-todo": "tsx scripts/saveTodoToObsidian.ts",
    "export-chat": "tsx scripts/exportCursorChatToObsidian.ts",
    "export-chat:debug": "tsx scripts/exportCursorChatToObsidian.ts --debug",
    "test": "playwright test",
    "test:embody": "playwright test tests/embody-user.spec.ts",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:visual": "playwright test tests/visual-regression.spec.ts",
    "test:visual:update": "playwright test tests/visual-regression.spec.ts --update-snapshots",
    "test:all": "playwright test && playwright test tests/visual-regression.spec.ts",
    "test:all:update": "playwright test && playwright test tests/visual-regression.spec.ts --update-snapshots",
    "test:unit": "jest --config jest.config.cjs",
    "test:unit:watch": "jest --config jest.config.cjs --watch",
    "test:unit:coverage": "jest --config jest.config.cjs --coverage",
    "screenshot:all": "tsx scripts/capture-screenshots-simple.ts",
    "verify-ui": "tsx scripts/verify-ui-flow.ts",
    "self-check": "tsx scripts/self_check.ts",
    "audit": "tsx scripts/audit_app.ts",
    "auto-fix": "tsx scripts/auto-test-fix-loop.ts"
  },
  "dependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/ios": "^8.0.2",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.89.0",
    "@zxing/library": "^0.21.3",
    "lucide-react": "^0.563.0",
    "puppeteer": "^24.36.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-window": "^2.2.6",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0"
  }
}
